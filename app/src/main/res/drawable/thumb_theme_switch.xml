<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp">
        <shape android:shape="oval">
            <solid android:color="?attr/colorSurface" />
            <stroke android:width="2dp" android:color="?attr/colorOnSecondaryContainer" />
        </shape>
    </item>

    <item android:gravity="center"
        android:drawable="@drawable/thumb_icons"
        android:top="10dp"
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"/>
</layer-list>